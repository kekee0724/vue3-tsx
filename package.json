  "license": "Apache-2.0",
  "directories": {
    "lib": "lib"
  },
  "private": false,
  "scripts": {
    "start": "vite --mode development",
    "build": "vuedx-typecheck . && vite build",
    "build:beta": "vue-tsc --noEmit && vite build --mode beta",
    "build:release": "vue-tsc --noEmit && vite build --mode release",
    "preview": "vite preview",
    "dev": "npm run docs && vite",
    "create": "ts-node build/create.ts && npm run docs",
    "docs": "ts-node build/check.ts && ts-node build/def.ts && ts-node build/docs.ts",
    "build-doc": "npm run docs && ts-node compile/build-components.ts && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "npm run docs && vue-cli-service build --target lib --name antdm --dest lib src/packages/index.ts",
    "remove": "ts-node build/remove.ts && npm run docs",
    "cdn": "npm run lib && ts-node build/cdn.ts",
    "compile": "npm run docs && ts-node compile/build-components.ts",
    "release": "npm run compile && npm publish"
  },
  "files": [
    "README.md",
    "types",
    "es"
  ],
  "publishConfig": {
    "registry": "https://nexus.aegis-info.com/repository/npm-aegis/"
