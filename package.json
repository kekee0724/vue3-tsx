{
  "name": "vite-react-app",
  "version": "0.0.0",
  "homepage": "./",
  "scripts": {
    "start": "vite --mode development",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "build:map": "react-scripts build",
    "build:beta": "tsc && vite build --mode beta",
    "build:release": "tsc && vite build --mode release",
    "preview": "vite preview"
  },
  "dependencies": {
    "antd-mobile": "^5.0.0-rc.17",
    "antd-mobile-icons": "^0.2.2",
    "axios": "^0.24.0",
    "dva": "2.6.0-beta.22",
    "jquery": "^3.6.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "moment": "^2.29.1",
    "qs": "^6.10.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.1.9"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.13",
    "@types/node": "^17.0.1",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.2",
    "@vitejs/plugin-react": "^1.0.7",
    "cross-env": "^7.0.3",
    "react-scripts": "5.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.0",
    "webpack-encoding-plugin": "^0.3.1"
  },
  "copy": [
    {
      "from": "src/assets/ipark.apk",
      "to": "assets"
    },
    {
      "from": "src/assets/reco.config.js",
      "to": "assets"
    }
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
